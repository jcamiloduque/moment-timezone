{
	"name": "moment-timezone",
	"version": "0.5.17",
	"description": "Parse and display moments in any timezone.",
	"homepage": "http://momentjs.com/timezone/",
	"author": "Tim Wood <washwithcare@gmail.com> (http://timwoodcreates.com/)",
	"keywords": [
		"moment",
		"date",
		"time",
		"timezone",
		"olson",
		"iana",
		"zone",
		"tz"
	],
	"main": "./index.js",
	"engines": {
		"node": "*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/moment/moment-timezone.git"
	},
	"bugs": {
		"url": "https://github.com/moment/moment-timezone/issues"
	},
	"license": "MIT",
	"dependencies": {
		"moment": ">= 2.9.0"
	},
	"devDependencies": {
		"grunt": "^1.0.1",
		"grunt-contrib-clean": "^1.1.0",
		"grunt-contrib-jshint": "^1.1.0",
		"grunt-contrib-nodeunit": "^1.0.0",
		"grunt-contrib-uglify": "^3.1.0"
	},
	"jspm": {
		"main": "builds/moment-timezone-with-data",
		"shim": {
			"moment-timezone": {
				"deps": ["moment"]
			}
		}
	},
	"scripts": {
		"test": "grunt",
		"test:guess:by:offset": "nodeunit ./tests/zones/**/*.js -t 'guess:by:offset'",
		"test:guess:by:abbr": "nodeunit ./tests/zones/**/*.js -t 'guess:by:abbr'",
		"test:guess": "npm run test:guess:by:offset && npm run test:guess:by:abbr"
	}
}
